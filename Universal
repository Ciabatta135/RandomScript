	-- SCRIPT PER DISATTIVARE CON TAB E ALT

	local MobileHideButton = {
		["_MOBILEBUTTON"] = Instance.new("ScreenGui");
		["_Button"] = Instance.new("ImageButton");
		["_UICorner"] = Instance.new("UICorner");
	}

	MobileHideButton["_MOBILEBUTTON"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	MobileHideButton["_MOBILEBUTTON"].Name = "MOBILEBUTTON"
	MobileHideButton["_MOBILEBUTTON"].Parent = game:GetService("CoreGui")

	MobileHideButton["_Button"].BackgroundColor3 = Color3.fromRGB(0, 187.00000405311584, 255)
	MobileHideButton["_Button"].BorderColor3 = Color3.fromRGB(0, 0, 0)
	MobileHideButton["_Button"].BorderSizePixel = 0
	MobileHideButton["_Button"].Position = UDim2.new(0.824757636, 0, 0.935643554, 0)
	MobileHideButton["_Button"].Size = UDim2.new(0.0193885155, 0, 0.0412541255, 0)
	MobileHideButton["_Button"].Name = "Button"
	MobileHideButton["_Button"].Parent = MobileHideButton["_MOBILEBUTTON"]

	MobileHideButton["_UICorner"].CornerRadius = UDim.new(1, 0)
	MobileHideButton["_UICorner"].Parent = MobileHideButton["_Button"]


	local function HideWIthTab(key)
		local UIS = game:GetService("UserInputService")
		if key.KeyCode == Enum.KeyCode.Tab or key.KeyCode == Enum.KeyCode.LeftAlt then
			SCREENGUI1["_UNIVERSAL"].Visible = not SCREENGUI1["_UNIVERSAL"].Visible
			SCREENGUI2["_UNIVERSAL2"].Visible = not SCREENGUI2["_UNIVERSAL2"].Visible
			AUTOKILLGUI["_Frame"].Visible = not AUTOKILLGUI["_Frame"].Visible
			Converted1["_UNIVERSAL3"].Visible = not Converted1["_UNIVERSAL3"].Visible
			MobileHideButton["_Button"].Visible = not MobileHideButton["_Button"].Visible
		end
	end

	game:GetService("UserInputService").InputBegan:Connect(HideWIthTab)


	local function HideWithMobile()
		SCREENGUI1["_UNIVERSAL"].Visible = not SCREENGUI1["_UNIVERSAL"].Visible
		SCREENGUI2["_UNIVERSAL2"].Visible = not SCREENGUI2["_UNIVERSAL2"].Visible
		AUTOKILLGUI["_Frame"].Visible = not AUTOKILLGUI["_Frame"].Visible
		Converted1["_UNIVERSAL3"].Visible = not Converted1["_UNIVERSAL3"].Visible
	end

	MobileHideButton["_Button"].MouseButton1Click:Connect(HideWithMobile)

