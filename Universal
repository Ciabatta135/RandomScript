	SCREENGUI1["_TELEPORTADVISE"].Parent = SCREENGUI1["_COOLTHINGTELEPORT"]

	SCREENGUI1["_UIStroke9"].Thickness = 3
	SCREENGUI1["_UIStroke9"].Parent = SCREENGUI1["_TELEPORTADVISE"]



	local tweenService = game:GetService("TweenService")

	local ti = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

	local TweenExpand = tweenService:Create(SCREENGUI1["_THEFRAME"], ti, {Size = UDim2.new(1, 0,10.164, 0)})
	local TweenContract = tweenService:Create(SCREENGUI1["_THEFRAME"], ti, {Size = UDim2.new(1, 0,0.017, 0)})

	local isExpanded = false

	local function TweenCreate()
		if isExpanded then
			TweenContract:Play()
			TweenContract.Completed:Wait()
			SCREENGUI1["_THEFRAME"].Visible = false
		else
			TweenExpand:Play()
			SCREENGUI1["_THEFRAME"].Visible = true
		end
		isExpanded = not isExpanded
	end

	SCREENGUI1["_CLOSETAB"].MouseButton1Click:Connect(TweenCreate)


	local function initializeNoclip(character)
		local player = game.Players.LocalPlayer
		local userInputService = game:GetService("UserInputService")

		local noclipButton = SCREENGUI1["_Noclip"]
		if not noclipButton then
			warn("Noclip button not found")
			return
		end

		local noclipEnabled = false

		local function updateNoclip()
			while noclipEnabled do
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
				wait(0.1)
			end

			for _, part in pairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.CanCollide = true
				end
			end
		end

		local function toggleNoclip()
			noclipEnabled = not noclipEnabled
			if noclipEnabled then
				coroutine.wrap(updateNoclip)()
			else
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = true
					end
				end
			end
		end

		userInputService.InputBegan:Connect(function(input, gameProcessed)
			if not gameProcessed and input.KeyCode == Enum.KeyCode.B then
				toggleNoclip()
			end
		end)

		noclipButton.MouseButton1Click:Connect(toggleNoclip)
	end

	local function onCharacterAdded(character)
		initializeNoclip(character)
	end

	local player = game.Players.LocalPlayer
	player.CharacterAdded:Connect(onCharacterAdded)

	if player.Character then
		initializeNoclip(player.Character)
	end


	local function teleport()
		local Players = game:GetService("Players")
		local UserInputService = game:GetService("UserInputService")
		local TeleportButton = SCREENGUI1["_ClickToTeleport"]
		local UsernameTextBox = SCREENGUI1["_TextBox"]


		local function findPlayerByNameOrPartial(partialName)
			local lowerPartialName = partialName:lower()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Name:lower():find(lowerPartialName) or player.DisplayName:lower():find(lowerPartialName) then
					print("Found player:", player.Name, "(DisplayName:", player.DisplayName, ")")
					return player
				end
			end
			return nil
		end


		local function teleportToPlayer(targetPlayerName)
			local localPlayer = Players.LocalPlayer
			local targetPlayer = findPlayerByNameOrPartial(targetPlayerName)

			if targetPlayer then
				local character = localPlayer.Character
				local targetCharacter = targetPlayer.Character

				if character and targetCharacter then
					local humanoidRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
					if humanoidRootPart then
						local playerHRP = character:FindFirstChild("HumanoidRootPart")
						if playerHRP then
							playerHRP.CFrame = humanoidRootPart.CFrame
						else
							print("Local player's HumanoidRootPart not found.")
						end
					else
						print("Target player's HumanoidRootPart not found.")
					end
				else
					print("Character or target character not found.")
				end
			else
				print("Player not found.")
			end
		end


		local function onTeleportButtonClick()
			local username = UsernameTextBox.Text
			if username and username ~= "" then
				teleportToPlayer(username)
			else
				print("No username entered.")
			end
		end


		TeleportButton.MouseButton1Click:Connect(onTeleportButtonClick)


		local function onInputBegan(input, gameProcessed)
			if gameProcessed then return end

			if input.KeyCode == Enum.KeyCode.Return then
				onTeleportButtonClick()
			elseif input.KeyCode == Enum.KeyCode.OAccentGrave then 
				UsernameTextBox:CaptureFocus()
			end
		end

		UserInputService.InputBegan:Connect(onInputBegan)
	end

	teleport()


	local function Spostare()
		local frame = SCREENGUI1["_UNIVERSAL"]
		local dragging = false
		local dragStart = nil
		local startPos = nil

		local function update(input)
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local moveConnection
				moveConnection = game:GetService("UserInputService").InputChanged:Connect(function(newInput)
					if dragging and newInput == input then
						update(newInput)
					elseif not dragging then
						moveConnection:Disconnect()
					end
				end)
			end
		end)
	end


	Spostare()


