	local function SpostareAltraGUI()
		local frame = SCREENGUI2["_UNIVERSAL2"]
		local dragging = false
		local dragStart = nil
		local startPos = nil

		local function update(input)
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local moveConnection
				moveConnection = game:GetService("UserInputService").InputChanged:Connect(function(newInput)
					if dragging and newInput == input then
						update(newInput)
					elseif not dragging then
						moveConnection:Disconnect()
					end
				end)
			end
		end)
	end


	local isSpectating = false 
	local lastTargetPlayer = nil

	local function spectatePlayer(targetPlayer)
		local camera = workspace.CurrentCamera
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
			camera.CameraSubject = targetPlayer.Character.Humanoid
			camera.CameraType = Enum.CameraType.Scriptable
		else
			camera.CameraType = Enum.CameraType.Custom
			camera.CameraSubject = nil
		end
	end

	local function onSpectateButtonClicked()
		local Players = game:GetService("Players")
		local UsernameTextBox = SCREENGUI2["_ViewBox"]
		local TeleportButton = SCREENGUI2["_CLICKTOVIEW"]

		local usernameOrDisplayName = UsernameTextBox.Text:lower()
		local targetPlayer = nil

		-- If currently spectating, stop spectating
		if isSpectating then
			spectatePlayer(nil)
			isSpectating = false
			lastTargetPlayer = nil
			print("Reset to default view.")
			return
		end

		-- Find the player that matches the partial name
		for _, player in pairs(Players:GetPlayers()) do
			if string.find(player.Name:lower(), usernameOrDisplayName) or string.find(player.DisplayName:lower(), usernameOrDisplayName) then
				targetPlayer = player
				break
			end
		end

		-- If a player is found, start spectating them
		if targetPlayer then
			spectatePlayer(targetPlayer)
			isSpectating = true
			lastTargetPlayer = targetPlayer
			print("Spectating player: " .. targetPlayer.Name)
		else
			print("Player with username/display name containing '" .. usernameOrDisplayName .. "' not found.")
		end
	end

	SCREENGUI2["_CLICKTOVIEW"].MouseButton1Click:Connect(onSpectateButtonClicked)


	SpostareAltraGUI()


	local tweenService = game:GetService("TweenService")

	local ti = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

	local TweenExpand = tweenService:Create(SCREENGUI2["_THEFRAME2"], ti, {Size = UDim2.new(1, 0,10.164, 0)})
	local TweenContract = tweenService:Create(SCREENGUI2["_THEFRAME2"], ti, {Size = UDim2.new(1, 0,0.017, 0)})

	local isExpanded = false

	local function TweenCreate2()
		if isExpanded then
			TweenContract:Play()
			TweenContract.Completed:Wait()
			SCREENGUI2["_THEFRAME2"].Visible = false
		else
			TweenExpand:Play()
			SCREENGUI2["_THEFRAME2"].Visible = true
		end
		isExpanded = not isExpanded
	end

	SCREENGUI2["_CLOSETAB2"].MouseButton1Click:Connect(TweenCreate2)

	local ESPSUS = false  
	local espInstances = {} 

	local function ESP()
		ESPSUS = true  
		local player = game.Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")

		local function createESP(target, playerName)
			local billboardGui = Instance.new("BillboardGui")
			billboardGui.Name = "PlayerNameTag"
			billboardGui.Size = UDim2.new(2, 0, 1, 0)
			billboardGui.StudsOffset = Vector3.new(0, 5, 0)  

			local nameLabel = Instance.new("TextLabel")
			nameLabel.Text = playerName
			nameLabel.Font = Enum.Font.SourceSansBold
			nameLabel.TextSize = 20
			nameLabel.TextColor3 = Color3.new(1, 1, 1)
			nameLabel.BackgroundTransparency = 1
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.Parent = billboardGui

			billboardGui.Parent = target

			local esp = Instance.new("BoxHandleAdornment")
			esp.Size = target.Size * 1.1  
			esp.Adornee = target
			esp.AlwaysOnTop = true
			esp.ZIndex = 5
			esp.Transparency = 0.5
			esp.Color3 = Color3.fromRGB(255, 0, 0)  
			esp.Parent = target


			table.insert(espInstances, {target = target, gui = billboardGui, esp = esp})
		end

		local function updateESP()
			for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
				if otherPlayer ~= player then
					local otherCharacter = otherPlayer.Character
					if otherCharacter then
						local existingBillboard = otherCharacter:FindFirstChild("PlayerNameTag")
						if not existingBillboard then
							createESP(otherCharacter.HumanoidRootPart, otherPlayer.Name)
						end
					end
				end
			end
		end


		while ESPSUS do
			updateESP()
			wait(1)  
		end


		for _, instance in ipairs(espInstances) do
			instance.gui:Destroy()
			instance.esp:Destroy()
		end
		espInstances = {}  
	end

	local function TurnOnEsp()
		if not ESPSUS then  
			ESP()  
		else
			ESPSUS = false  
			for _, instance in ipairs(espInstances) do
				instance.gui:Destroy()
				instance.esp:Destroy()
			end
			espInstances = {}  
		end
	end


	SCREENGUI2["_ESP"].MouseButton1Click:Connect(TurnOnEsp)

