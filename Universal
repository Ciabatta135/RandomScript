	local function OpenSettings()
		Settings1["_THEFRAME"].Visible = true
	end

	local function CloseSettings()
		Settings1["_THEFRAME"].Visible = false
	end

	Settings1["_CloseButton"].MouseButton1Click:Connect(CloseSettings)
	SCREENGUI2["_FREECAM"].MouseButton1Click:Connect(OpenSettings)

	local function OpenThings()
		local function OpenGuiColors()
			Settings1["_GUICOLORS"].Visible = true
			Settings1["_BordersColors"].Visible = false
			Settings1["_KEYBINDS1"].Visible = false
			Settings1["_Others"].Visible = false
		end
		Settings1["_GuiColor"].MouseButton1Click:Connect(OpenGuiColors)

		local function OpenBordersColors()
			Settings1["_BordersColors"].Visible = true
			Settings1["_KEYBINDS1"].Visible = false
			Settings1["_Others"].Visible = false
			Settings1["_GUICOLORS"].Visible = false
		end
		Settings1["_BORDERSCOLOR"].MouseButton1Click:Connect(OpenBordersColors)

		local function OpenKeyBinds()
			Settings1["_KEYBINDS1"].Visible = true
			Settings1["_Others"].Visible = false
			Settings1["_GUICOLORS"].Visible = false
			Settings1["_BordersColors"].Visible = false
		end
		Settings1["_KEYBINDS"].MouseButton1Click:Connect(OpenKeyBinds)

		local function OpenOthers()
			Settings1["_KEYBINDS1"].Visible = false
			Settings1["_Others"].Visible = true
			Settings1["_GUICOLORS"].Visible = false
			Settings1["_BordersColors"].Visible = false
		end
		Settings1["_Other"].MouseButton1Click:Connect(OpenOthers)
	end

	OpenThings()

	local function ChangeGuiColors()
		local function ChangeToWhite()
			SCREENGUI1["_THEFRAME"].BackgroundColor3 = Color3.new(1, 1, 1)
			SCREENGUI2["_THEFRAME2"].BackgroundColor3 = Color3.new(1, 1, 1)
			AUTOKILLGUI["_Frame"].BackgroundColor3 = Color3.new(1, 1, 1)
			Converted1["_THEFRAME3"].BackgroundColor3 = Color3.new(1, 1, 1)
			Settings1["_THEFRAME"].BackgroundColor3 = Color3.new(1, 1, 1)
			Converted["_FREIM"].BackgroundColor3 = Color3.new(1, 1, 1)
		end
		Settings1["_White"].MouseButton1Click:Connect(ChangeToWhite)

		local function ChangeGuiToYellow()
			SCREENGUI1["_THEFRAME"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
			SCREENGUI2["_THEFRAME2"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
			AUTOKILLGUI["_Frame"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
			Converted1["_THEFRAME3"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
			Settings1["_THEFRAME"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
			Converted["_FREIM"].BackgroundColor3 = Color3.new(0.917647, 1, 0)
		end
		Settings1["_Yellow"].MouseButton1Click:Connect(ChangeGuiToYellow)

		local function ChangeGuiToOrange()
			SCREENGUI1["_THEFRAME"].BackgroundColor3 = Color3.new(1, 0.501961, 0)
