	-- SPOSTARE GUI DEI SETTINGS

	local function SpostareGuiDeiSettings()
		local frame = Settings1["_THEFRAME"]
		local dragging = false
		local dragStart = nil
		local startPos = nil

		local function update(input)
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local moveConnection
				moveConnection = game:GetService("UserInputService").InputChanged:Connect(function(newInput)
					if dragging and newInput == input then
						update(newInput)
					elseif not dragging then
						moveConnection:Disconnect()
					end
				end)
			end
		end)
	end

	Settings1["_RejoinButton"].MouseButton1Click:Connect(function()
		game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, plr)
	end)


	SpostareGuiDeiSettings()

	-- Funzioni 

	local function fly()
		local Players = game:GetService("Players")
		local UserInputService = game:GetService("UserInputService")
		local ContextActionService = game:GetService("ContextActionService")
		local RunService = game:GetService("RunService")

		local player = Players.LocalPlayer
		local camera = workspace.CurrentCamera

		local bodyGyro = Instance.new("BodyGyro")
		bodyGyro.maxTorque = Vector3.new(1, 1, 1) * 10^6
		bodyGyro.P = 10^6

		local bodyVel = Instance.new("BodyVelocity")
		bodyVel.maxForce = Vector3.new(1, 1, 1) * 10^6
		bodyVel.P = 10^4

		local isFlying = false
		local isRunning = false
		local movement = {forward = 0, backward = 0, right = 0, left = 0, up = 0, down = 0}

		local flySpeedTextBox = Settings1["_FLYSPEED1"]
		local defaultFlySpeed = tonumber(flySpeedTextBox.Text) or 4
		local runFlySpeed = defaultFlySpeed * 50.75

		-- Variables for animation
		local humanoid
		local track

		-- Animation setup
		local Anim = Instance.new("Animation")
		Anim.AnimationId = "rbxassetid://95101792406607" -- Replace with your idle animation ID
		local freezeAtTime = 2  -- Time (in seconds) to freeze the animation after starting

		local function setFlying(flying)
            isFlying = flying
            local character = player.Character
            if character then
                local hrp = character:FindFirstChild("HumanoidRootPart")
                humanoid = character:FindFirstChild("Humanoid")
                if hrp and humanoid then
                    bodyGyro.Parent = isFlying and hrp or nil
                    bodyVel.Parent = isFlying and hrp or nil
                    bodyGyro.CFrame = hrp.CFrame
                    bodyVel.Velocity = Vector3.new()
                    if isFlying then
                        humanoid.PlatformStand = true
                        if track then
                            track:Play()
                            track.Looped = true -- ensure it loops
                        end
                    else
                        humanoid.PlatformStand = false
                        if track then
                            track:Stop()
                        end
                    end
                end
            end
        end


		local function onUpdate(dt)
			if isFlying then
				local character = player.Character
				if character then
					local humanoid = character:FindFirstChildOfClass("Humanoid")
					if humanoid then
						if humanoid:GetState() == Enum.HumanoidStateType.Landed then
							setFlying(false)
							return
						end
					end
				end

				local cf = camera.CFrame
				local direction = cf.rightVector * (movement.right - movement.left) + cf.lookVector * (movement.forward - movement.backward) + Vector3.new(0, movement.up - movement.down, 0)
				if direction.Magnitude > 0 then
					direction = direction.unit
				end

				-- Maintain a stable camera by only updating the BodyGyro's CFrame when flying
				if isFlying then
					bodyGyro.CFrame = CFrame.new(camera.CFrame.Position, camera.CFrame.Position + camera.CFrame.LookVector)
				end

				-- Apply floating effect by oscillating the vertical velocity
				local time = tick()
				local floatOffset = math.sin(time * 3) * 2  -- Adjust the frequency and amplitude as needed
				bodyVel.Velocity = direction * player.Character.Humanoid.WalkSpeed * (isRunning and runFlySpeed or defaultFlySpeed) + Vector3.new(0, floatOffset, 0)
			end
		end

		local function onKeyPress(input, gameProcessedEvent)
			if input.KeyCode == Enum.KeyCode.E and not gameProcessedEvent then
				if player.Character and player.Character:FindFirstChild("Humanoid") then
					local humanoid = player.Character.Humanoid
					if humanoid:GetState() == Enum.HumanoidStateType.Dead then
						return
					end
					setFlying(not isFlying)
				end
			end

			if input.KeyCode == Enum.KeyCode.LeftShift then
				isRunning = true
			end
		end

		local function onKeyRelease(input, gameProcessedEvent)
			if input.KeyCode == Enum.KeyCode.LeftShift then
				isRunning = false
			end
		end

		local function movementBind(actionName, inputState, inputObject)
			if inputState == Enum.UserInputState.Begin then
				movement[actionName] = 1
			elseif inputState == Enum.UserInputState.End then
				movement[actionName] = 0
			end
			return Enum.ContextActionResult.Pass
		end

		-- Function to update the animation and set it up when character respawns
		local function setupAnimation()
			humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
			if humanoid then
				track = humanoid:LoadAnimation(Anim)
			end
		end

		local function onCharacterAdded(character)
			character:WaitForChild("HumanoidRootPart")
			if isFlying then
				-- Stop flying on respawn
				setFlying(false)
			end
			setupAnimation()  -- Set up the animation for the new character
		end

		player.CharacterAdded:Connect(onCharacterAdded)

		if player.Character then
			onCharacterAdded(player.Character)  -- Initialize on the first spawn
		end

		UserInputService.InputBegan:Connect(onKeyPress)
		UserInputService.InputEnded:Connect(onKeyRelease)
		ContextActionService:BindAction("forward", movementBind, false, Enum.PlayerActions.CharacterForward)
		ContextActionService:BindAction("backward", movementBind, false, Enum.PlayerActions.CharacterBackward)
		ContextActionService:BindAction("left", movementBind, false, Enum.PlayerActions.CharacterLeft)
		ContextActionService:BindAction("right", movementBind, false, Enum.PlayerActions.CharacterRight)
		ContextActionService:BindAction("up", movementBind, false, Enum.KeyCode.Space)
		ContextActionService:BindAction("down", movementBind, false, Enum.KeyCode.LeftControl)
		RunService.RenderStepped:Connect(onUpdate)



		local function updateFlySpeed()
			local speed = tonumber(flySpeedTextBox.Text)
			if speed then
				defaultFlySpeed = speed
				runFlySpeed = defaultFlySpeed * 50.75
			end
		end

		flySpeedTextBox.FocusLost:Connect(updateFlySpeed)

		SCREENGUI1["_Fly"].MouseButton1Click:Connect(function()
			setFlying(not isFlying)
		end)
	end

	fly()





	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players")

	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local rootPart = character:WaitForChild("HumanoidRootPart")

	-- Variables for spin speed and state
	local spinSpeed = tonumber(Settings1["_SpinSpeed1"].Text) or 100
	local spinning = false

	-- Function to start spinning
	local function startSpinning()
		-- Check if there's already a spinning part and remove it
		for _, v in pairs(rootPart:GetChildren()) do
			if v.Name == "Spinning" then
				v:Destroy()
			end
		end

		-- Create and apply BodyAngularVelocity
		local spin = Instance.new("BodyAngularVelocity")
		spin.Name = "Spinning"
		spin.Parent = rootPart
		spin.MaxTorque = Vector3.new(0, math.huge, 0)
		spin.AngularVelocity = Vector3.new(0, spinSpeed, 0)

		spinning = true
	end

	-- Function to stop spinning
	local function stopSpinning()
		-- Remove the spinning part if it exists
		for _, v in pairs(rootPart:GetChildren()) do
			if v.Name == "Spinning" then
				v:Destroy()
			end
		end

		spinning = false
	end

	-- Function to update spin speed
	local function updateSpinSpeed()
		local speed = tonumber(Settings1["_SpinSpeed1"].Text)
		if speed then
			spinSpeed = speed
		end
	end

	Settings1["_SpinSpeed1"].FocusLost:Connect(updateSpinSpeed)

	-- Function to handle character respawn or spawn
	local function onCharacterAdded(newCharacter)
		character = newCharacter
		rootPart = newCharacter:WaitForChild("HumanoidRootPart")

		-- Stop spinning if player resets
		stopSpinning()
	end

	-- Call onCharacterAdded for the initial spawn
	onCharacterAdded(character)

	-- Listen for character respawn
	player.CharacterAdded:Connect(onCharacterAdded)

	-- Key press detection
	UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
		if gameProcessedEvent then return end -- Ignore input if the game is processing it

		if input.KeyCode == Enum.KeyCode.V then
			if spinning then
				stopSpinning()
			else
				startSpinning()
			end
		end
	end)

	-- Button click detection
	SCREENGUI1["_Spin"].MouseButton1Click:Connect(function()
		if spinning then
			stopSpinning()
		else
			startSpinning()
		end
	end)



	-- SUPER SPEED

	local function Speed(input)
		local UIS = game:GetService("UserInputService")

		local player = game.Players.LocalPlayer
		local defaultWalkSpeed = 16
		local boostedWalkSpeed = tonumber(Settings1["_SPEED1"].Text) or 300
		local isBoosted = false

		local function toggleSpeed()
			local character = player.Character
			local humanoid = character and character:FindFirstChildOfClass("Humanoid")

			if humanoid then
				if isBoosted then
					humanoid.WalkSpeed = defaultWalkSpeed
				else
					humanoid.WalkSpeed = boostedWalkSpeed
				end
				isBoosted = not isBoosted
			end
		end

		local function onInputBegan(input, gameProcessed)
			if gameProcessed then return end

			if input.KeyCode == Enum.KeyCode.Z then
				toggleSpeed()
			end
		end

		UIS.InputBegan:Connect(onInputBegan)
		SCREENGUI1["_Speed"].MouseButton1Click:Connect(toggleSpeed)

		local function updateSpeed()
			local speed = tonumber(Settings1["_SPEED1"].Text)
			if speed then
				boostedWalkSpeed = speed
			end
		end

		Settings1["_SPEED1"].FocusLost:Connect(updateSpeed)
	end

	Speed()

	-- Some settings Cool Functions

	local function Leave()
		game.Players.LocalPlayer:Kick("You wanted to leave")
	end

	Settings1["_LeaveServerGame"].MouseButton1Click:Connect(Leave)


	-- Bell Tween
	local function FULLHIDETHING()
		local TweenService = game:GetService("TweenService")
		local button = Settings1["_RealButtonToDisable"]
		local frame = Settings1["_ImmineTween"]

		local tweenInfo = TweenInfo.new(.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
		local positionRight = UDim2.new(0.6, 0, 0.086, 0)
		local positionLeft = UDim2.new(0.098, 0, 0.086, 0)

		local originalColor = Color3.fromRGB(91, 91, 91)
		local toggleColor = Color3.new(0, 1, 0.717647)

		local isToggled = false

		local tweenToRight = TweenService:Create(frame, tweenInfo, {Position = positionRight})
		local tweenToLeft = TweenService:Create(frame, tweenInfo, {Position = positionLeft})

		local function onButtonClick()
			if isToggled then
				tweenToLeft:Play()
				tweenToLeft.Completed:Connect(function()
					button.BackgroundColor3 = originalColor
				end)
			else
				tweenToRight:Play()
				tweenToRight.Completed:Connect(function()
					button.BackgroundColor3 = toggleColor
				end)
			end

			isToggled = not isToggled
		end

		button.MouseButton1Click:Connect(onButtonClick)

		local function ActuallyHideThat()
			MobileHideButton["_Button"].Visible = not MobileHideButton["_Button"].Visible
		end

		button.MouseButton1Click:Connect(ActuallyHideThat)
	end

	FULLHIDETHING()

