		local Players = game:GetService("Players")
		local plr = game.Players.LocalPlayer

		local function TeleportPlayerToAllPlayer()
			for _, OtherPlayers in ipairs(Players:GetPlayers()) do
				plr.Character.HumanoidRootPart.CFrame = OtherPlayers.Character.HumanoidRootPart.CFrame
				wait(0.00001)
			end
		end

		Converted["_CRAZY"].MouseButton1Click:Connect(TeleportPlayerToAllPlayer)


		-- LocalScript inside StarterPlayerScripts

		-- Reference to the player and character
		local player = game.Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()

		-- Variable to manage the freeze state
		local isFrozen = false

		-- Function to toggle the freeze state
		local function freezePlayer()
			if character then
				for _, part in pairs(character:GetChildren()) do
					-- Check if the part is a BasePart (all physical parts in Roblox)
					if part:IsA("BasePart") then
						part.Anchored = not isFrozen  -- Toggle the anchoring state
					end
				end
				isFrozen = not isFrozen  -- Update the freeze state
			end
		end

		-- Connect the button click event to the freeze function
		Converted["_FREEZE"].MouseButton1Click:Connect(freezePlayer)

		-- Function to handle key press
		local function onKeyPress(input)
			if input.KeyCode == Enum.KeyCode.K then
				freezePlayer()
			end
		end

		-- Connect the key press event to the function
		game:GetService("UserInputService").InputBegan:Connect(onKeyPress)

		-- Handle character respawn to maintain the freeze state
		player.CharacterAdded:Connect(function(char)
			character = char
			if isFrozen then
				freezePlayer()
			end
		end)




		local function DisableSit()
			-- Disable Sitting for a player
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoid = character:WaitForChild("Humanoid")

			local isPreventingSit = false -- Variable to track whether sitting is being prevented

			-- Function to toggle sitting prevention
			local function toggleSitPrevention()
				isPreventingSit = not isPreventingSit -- Toggle the state

				if isPreventingSit then
					-- Prevent sitting
					local function preventSitting(_, newState)
						if newState == Enum.HumanoidStateType.Seated then
							humanoid.Sit = false  -- Force the humanoid out of the seated state
						end
					end

					-- Connect the function to the StateChanged event
					humanoid.StateChanged:Connect(preventSitting)
				else
					-- Remove the sitting prevention functionality
					-- Note: Disconnection of StateChanged might not be straightforward here, so you might need a more sophisticated approach for clean up
				end
			end

			-- Assuming Converted["_NOSIT"] is a GUI button or similar
			if Converted and Converted["_NOSIT"] then
				Converted["_NOSIT"].MouseButton1Click:Connect(toggleSitPrevention)
			end
		end

		DisableSit()


		-- LocalScript inside StarterPlayerScripts

		-- Variables to control the teleport distance, detection range, and teleportation status
		local teleportDistance = 7
		local detectionRange = 6
		local isTeleportationEnabled = true

		-- Create the UI button
		local screenGui = Instance.new("ScreenGui")
		screenGui.Name = "TeleportationGui"
		screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

		local toggleButton = Instance.new("TextButton")
		toggleButton.Name = "ToggleButton"
		toggleButton.Text = "Toggle Teleport"
		toggleButton.Size = UDim2.new(0, 200, 0, 50)
		toggleButton.Position = UDim2.new(0, 10, 0.5, -25)  -- Centered vertically on the left
		toggleButton.Parent = screenGui
		toggleButton.Visible = false

		-- Function to toggle teleportation
		local function toggleTeleportation()
			isTeleportationEnabled = not isTeleportationEnabled
			if isTeleportationEnabled then
				toggleButton.Text = "Disable Teleport"
			else
				toggleButton.Text = "Enable Teleport"
			end
		end


		Converted["_Spin"].MouseButton1Click:Connect(toggleTeleportation)

		-- Function to teleport the player in a random direction
		local function teleportRandomlyAndFaceTarget(targetPosition)
			local player = game.Players.LocalPlayer
			if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = player.Character.HumanoidRootPart
				local currentCFrame = humanoidRootPart.CFrame

				-- Choose a random direction
				local randomDirection = math.random(1, 4)
				local direction

				if randomDirection == 1 then
					direction = currentCFrame.RightVector
				elseif randomDirection == 2 then
					direction = -currentCFrame.RightVector
				elseif randomDirection == 3 then
					direction = currentCFrame.LookVector
				elseif randomDirection == 4 then
					direction = -currentCFrame.LookVector
				end

				-- Teleport the player in the chosen direction
				humanoidRootPart.CFrame = currentCFrame + direction * teleportDistance

				-- Adjust rotation to face the original target position
				local newCFrame = humanoidRootPart.CFrame
				local newLookVector = (targetPosition - newCFrame.Position).unit
				humanoidRootPart.CFrame = CFrame.new(newCFrame.Position, newCFrame.Position + newLookVector)
			end
		end

		-- Function to detect and handle nearby players
		local function detectNearbyPlayers()
			local player = game.Players.LocalPlayer
			if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = player.Character.HumanoidRootPart
				local players = game.Players:GetPlayers()

				for _, otherPlayer in ipairs(players) do
					if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
						local otherHumanoidRootPart = otherPlayer.Character.HumanoidRootPart
						local distance = (humanoidRootPart.Position - otherHumanoidRootPart.Position).magnitude

						if distance < detectionRange and isTeleportationEnabled then
							teleportRandomlyAndFaceTarget(otherHumanoidRootPart.Position)
							break
						end
					end
				end
			end
		end

		local function DoDetect()
			while true do
				detectNearbyPlayers()
				wait(.1)  -- Check every second; adjust as needed
			end
		end
		task.spawn(DoDetect)



		local function HideWIthTab(key)
			local UIS = game:GetService("UserInputService")
			if key.KeyCode == Enum.KeyCode.Tab or key.KeyCode == Enum.KeyCode.LeftAlt then
				Converted["_UNIVERSAL"].Visible = not Converted["_UNIVERSAL"].Visible
			end
		end

		game:GetService("UserInputService").InputBegan:Connect(HideWIthTab)

	end

	OtherGuiCrazy()


