	AVVISOGUI["_CHAT"].Text = "Hi, script got executed correctly, press Tab of Left Alt to hide the Gui"
	AVVISOGUI["_CHAT"].TextColor3 = Color3.fromRGB(152.0000061392784, 152.0000061392784, 152.0000061392784)
	AVVISOGUI["_CHAT"].TextScaled = true
	AVVISOGUI["_CHAT"].TextSize = 14
	AVVISOGUI["_CHAT"].TextWrapped = true
	AVVISOGUI["_CHAT"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	AVVISOGUI["_CHAT"].BackgroundTransparency = 1
	AVVISOGUI["_CHAT"].BorderColor3 = Color3.fromRGB(0, 0, 0)
	AVVISOGUI["_CHAT"].BorderSizePixel = 0
	AVVISOGUI["_CHAT"].Position = UDim2.new(0.0350876115, 0, 0.341463417, 0)
	AVVISOGUI["_CHAT"].Size = UDim2.new(0.92458111, 0, 0.463415235, 0)
	AVVISOGUI["_CHAT"].Name = "CHAT"
	AVVISOGUI["_CHAT"].Parent = AVVISOGUI["_FRAMEAVVISO"]

	local TweenService = game:GetService("TweenService")

	local TweenInfoForInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

	local TweenOnExecute = TweenService:Create(AVVISOGUI["_FRAMEAVVISO"], TweenInfoForInfo, {Position = UDim2.new(0.026, 0,0.822, 0)})
	local TweenOnExecute2 = TweenService:Create(AVVISOGUI["_FRAMEAVVISO"], TweenInfoForInfo, {Position = UDim2.new(-0.5, 0,0.822, 0)})

	local TweenButFast = TweenService:Create(AVVISOGUI["_FRAMEAVVISO"], TweenInfo.new(.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Position = UDim2.new(0.026, 0,0.822, 0)})


	local function ApparizioneOnExecute()
		TweenOnExecute:Play()
		TweenOnExecute.Completed:Wait()
		wait(3.5)
		TweenOnExecute2:Play()
	end

	ApparizioneOnExecute()

	local guiHidden = false -- This variable will keep track of the GUI state

	local function AvvisoOnHide(input)
		if input.KeyCode == Enum.KeyCode.Tab or input.KeyCode == Enum.KeyCode.LeftAlt then
			guiHidden = not guiHidden -- Toggle the GUI state
			if guiHidden then
				AVVISOGUI["_CHAT"].Text = "Gui hidden successfully"
				TweenButFast:Play()
				TweenButFast.Completed:Wait()
				wait(.5)
				TweenService:Create(AVVISOGUI["_FRAMEAVVISO"], TweenInfo.new(.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Position = UDim2.new(-0.5, 0,0.822, 0)}):Play()
			else
				AVVISOGUI["_CHAT"].Text = "Gui shown successfully"
				TweenButFast:Play()
				TweenButFast.Completed:Wait()
				wait(.5)
				TweenService:Create(AVVISOGUI["_FRAMEAVVISO"], TweenInfo.new(.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Position = UDim2.new(-0.5, 0,0.822, 0)}):Play()
			end	
		end
	end

	game:GetService("UserInputService").InputBegan:Connect(AvvisoOnHide)

	local function BeANinja()
		-- LocalScript inside StarterPlayerScripts

		-- Variables to control the teleport distance
		local teleportDistance = 10  -- Adjust this value to control how far you teleport

		-- Function to teleport the player
		local function teleportPlayer(direction)
			local player = game.Players.LocalPlayer
			if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = player.Character.HumanoidRootPart
				local currentCFrame = humanoidRootPart.CFrame

				if direction == "right" then
					humanoidRootPart.CFrame = currentCFrame + currentCFrame.RightVector * teleportDistance
				elseif direction == "left" then
					humanoidRootPart.CFrame = currentCFrame - currentCFrame.RightVector * teleportDistance
				elseif direction == "behind" then
					humanoidRootPart.CFrame = currentCFrame - currentCFrame.LookVector * teleportDistance
				elseif direction == "forward" then
					humanoidRootPart.CFrame = currentCFrame + currentCFrame.LookVector * teleportDistance
				end
			end
		end

		-- Function to handle key presses
		local function onKeyPress(input)
			if input.KeyCode == Enum.KeyCode.Five then
				teleportPlayer("right")
			elseif input.KeyCode == Enum.KeyCode.Four then
				teleportPlayer("left")
			elseif input.KeyCode == Enum.KeyCode.Q then
				teleportPlayer("behind")
			elseif input.KeyCode == Enum.KeyCode.Six then
				teleportPlayer("forward")
			end
		end

		-- Connect the key press event to the function
		game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	end
	BeANinja()

end



local Plr = game.Players.LocalPlayer

local WhiteListedPersons = {2817513261, 2015322056, 2819479166, 2817515788, 8471315531, 2819427932}

local function ActiveWhiteList()
	if table.find(WhiteListedPersons, Plr.UserId) then
		MoreCredits()
		ScriptInGeneral()
	else
		Plr:Kick("You are not allowed to use this script, because ur not in the WhiteList")
	end
end

ActiveWhiteList()
