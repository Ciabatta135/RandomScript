			AUTOKILLGUI["_UIStroke"].Color = Color3.new(1, 0.321569, 1)
			AUTOKILLGUI["_TEEEXT"].BackgroundColor3 = Color3.new(1, 0.321569, 1)
			Converted1["_UIStroke1"].Color = Color3.new(1, 0.321569, 1)
			Converted1["_UNIVERSAL3"].BackgroundColor3 = Color3.new(1, 0.321569, 1)
			Converted1["_UIStroke2"].Color = Color3.new(1, 0.321569, 1)
			Settings1["_UIStroke"].Color = Color3.new(1, 0.321569, 1)
			Converted["_UIStroke"].Color = Color3.new(1, 0.321569, 1)
		end
		Settings1["_Purple1"].MouseButton1Click:Connect(ChangeGuiBordersPurple)

