	local function AUTOKILL()
		local Players = game:GetService("Players")
		local UserInputService = game:GetService("UserInputService")
		local RunService = game:GetService("RunService")

		local TeleportButton = AUTOKILLGUI["_START"]
		local StopButton = AUTOKILLGUI["_STOP"]
		local UsernameTextBox = AUTOKILLGUI["_TextBox"]

		local player = Players.LocalPlayer
		local spinSpeed = 10000
		local connection
		local isTeleporting = false

		local function findPlayerByNameOrPartial(partialName)
			local lowerPartialName = partialName:lower()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Name:lower():find(lowerPartialName) or player.DisplayName:lower():find(lowerPartialName) then
					print("Found player:", player.Name, "(DisplayName:", player.DisplayName, ")")
					return player
				end
			end
			return nil
		end

		local function teleportToPlayer(targetPlayer)
			local character = player.Character
			local targetCharacter = targetPlayer.Character

			if not character or not targetCharacter then return end
			if not character:FindFirstChild("HumanoidRootPart") or not targetCharacter:FindFirstChild("HumanoidRootPart") then return end
			if not targetCharacter:FindFirstChild("Humanoid") then return end
			isTeleporting = true

			local offsetDistance = 3 -- distance behind the target player

			while isTeleporting and not targetCharacter.Humanoid.Sit do
				-- Move behind the target by offsetDistance studs along the negative look vector (backwards)
				local behindCFrame = targetCharacter.HumanoidRootPart.CFrame * CFrame.new(0, 0, offsetDistance)
				player.Character.HumanoidRootPart.CFrame = behindCFrame
				local player = game.Players.LocalPlayer
				local backpack = player:WaitForChild("Backpack")

				local couchTool = backpack:FindFirstChild("Couch")
				if couchTool then
					couchTool.Parent = player.Character -- Grabs (equips) the tool
				else
					warn("Couch tool not found in Backpack.")
				end

				task.wait(0.1)
			end



			if isTeleporting then
				player.Character.HumanoidRootPart.CFrame = CFrame.new(2347823, 2374098, 239874)
				isTeleporting = false
				local player = game.Players.LocalPlayer
				local backpack = player:WaitForChild("Backpack")

				local couchTool = backpack:FindFirstChild("Couch")
				if couchTool then
					couchTool.Parent = nil
				else
					warn("Couch tool not found in Backpack.")
				end

			end

		end



		local function startAutoKill(targetPlayerName)
			local targetPlayer = findPlayerByNameOrPartial(targetPlayerName)

			if targetPlayer then
				teleportToPlayer(targetPlayer)
			end
		end

		local function StopAutoKill()
			isTeleporting = false
		end

		local function onTeleportButtonClick()
			local username = UsernameTextBox.Text
			if username and username ~= "" then
				startAutoKill(username)
			else
				print("No username entered.")
			end
		end



		StopButton.MouseButton1Click:Connect(StopAutoKill)
		TeleportButton.MouseButton1Click:Connect(onTeleportButtonClick)

	end

	AUTOKILL()


	local function SpostareGuiAutoKill()
		local frame = AUTOKILLGUI["_Frame"]
		local dragging = false
		local dragStart = nil
		local startPos = nil

		local function update(input)
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local moveConnection
				moveConnection = game:GetService("UserInputService").InputChanged:Connect(function(newInput)
					if dragging and newInput == input then
						update(newInput)
					elseif not dragging then
						moveConnection:Disconnect()
					end
				end)
			end
		end)
	end


	SpostareGuiAutoKill()


