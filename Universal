	Converted1["_UIStroke7"].Parent = Converted1["_SCRITTACHEDICE"]


	local tweenService = game:GetService("TweenService")

	local ti = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

	local TweenExpand = tweenService:Create(Converted1["_THEFRAME3"], ti, {Size = UDim2.new(1, 0,10.164, 0)})
	local TweenContract = tweenService:Create(Converted1["_THEFRAME3"], ti, {Size = UDim2.new(1, 0,0.017, 0)})

	local isExpanded = false

	local function TweenCreateForClient()
		if isExpanded then
			TweenContract:Play()
			TweenContract.Completed:Wait()
			Converted1["_THEFRAME3"].Visible = false
		else
			TweenExpand:Play()
			Converted1["_THEFRAME3"].Visible = true
		end
		isExpanded = not isExpanded
	end

	Converted1["_CLOSETAB3"].MouseButton1Click:Connect(TweenCreateForClient)

	local function NightDay()
		local Lighting = game:GetService("Lighting")

		local Day = true

		local function Change()
			if Day then Lighting.TimeOfDay = "00:00:00"
				Day = false
			else
				Lighting.TimeOfDay = "14:00:00"
				Day = true
			end
		end
		Converted1["_DAYNIGHT"].MouseButton1Click:Connect(Change)
	end

	NightDay()

	local function SpostareClientGui()
		local frame = Converted1["_UNIVERSAL3"]
		local dragging = false
		local dragStart = nil
		local startPos = nil

		local function update(input)
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local moveConnection
				moveConnection = game:GetService("UserInputService").InputChanged:Connect(function(newInput)
					if dragging and newInput == input then
						update(newInput)
					elseif not dragging then
						moveConnection:Disconnect()
					end
				end)
			end
		end)
	end


	SpostareClientGui()


