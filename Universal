local function MoreCredits()
	local Converted = {
		["_MORECREDITS"] = Instance.new("ScreenGui");
		["_ImageLabel"] = Instance.new("ImageLabel");
	}

	Converted["_MORECREDITS"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	Converted["_MORECREDITS"].Name = "MORECREDITS"
	Converted["_MORECREDITS"].Parent = game:GetService("CoreGui")

	Converted["_ImageLabel"].Image = "http://www.roblox.com/asset/?id=18637687337"
	Converted["_ImageLabel"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Converted["_ImageLabel"].BackgroundTransparency = 1
	Converted["_ImageLabel"].BorderColor3 = Color3.fromRGB(0, 0, 0)
	Converted["_ImageLabel"].BorderSizePixel = 0
	Converted["_ImageLabel"].Position = UDim2.new(0.28256005, 0, -0.95224756, 0)
	Converted["_ImageLabel"].Size = UDim2.new(0.383296043, 0, 0.952145219, 0)
	Converted["_ImageLabel"].Parent = Converted["_MORECREDITS"]


	local TweenService = game:GetService("TweenService")

	local TweenInformations = TweenInfo.new(3, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out)
	local Tween2Informations = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

	local Tween1 = TweenService:Create(Converted["_ImageLabel"], TweenInformations, {Position = UDim2.new(0.293, 0,0.023, 0)})
	local Tween2 = TweenService:Create(Converted["_ImageLabel"], Tween2Informations, {Position = UDim2.new(0.283, 0,-0.952, 0)})

	local function ActiveTween()
		Tween1:Play()
		Tween1.Completed:Wait()
		wait(.5)
		Tween2:Play()
	end

	ActiveTween()

end

task.spawn(function()
	local args = {
		[1] = "PickingTools",
		[2] = "Couch"
	}

	game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
end)




